# c-project
#include<stdio.h>
int main()
{
    int n,i;
    printf("enter the fibonacci value\n");
    scanf("%d",&n);
    for(i=0;i<=n;i++)
    {

         printf("%d\n",fib(i));

    }


}
int fib(int n)
{
    if(n==0)
        return 0;

    else if(n==1){
        return 1;
    }

    else
        return (fib(n-1)+fib(n-2));
}
